<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="HomeTarefaFinalizada" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0083FC;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerTarefaFinalizada">
   <children>
      <MenuBar layoutY="-2.0" prefHeight="23.0" prefWidth="600.0">
         <menus>
            <Menu mnemonicParsing="false">
               <items>
                  <MenuItem fx:id="criarTarefa" mnemonicParsing="false" onAction="#handleOnActionCriarTarefa" text="Criar Tarefa" />
                  <MenuItem fx:id="tarefaFinalizada" mnemonicParsing="false" onAction="#handleOnActionExcluirTarefa" text="Excluir Tarefa" />
                  <MenuItem fx:id="consultar" mnemonicParsing="false" onAction="#handleOnActionConsultar" text="Consultar Tarefa" />
                  <MenuItem fx:id="sair" mnemonicParsing="false" onAction="#handleOnActionSair" text="Sair" />
               </items>
               <graphic>
                  <ImageView fitHeight="25.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@menu-button-of-three-horizontal-lines.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
